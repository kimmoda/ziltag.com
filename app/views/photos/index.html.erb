<div class="container-fluid layout_fluid" data-layout="outer">
    
  <div class="fill_white" data-layout="outer_fill_white"></div>
  <div class="custom_header_line" data-layout="custom_header_line"></div>
  
  <div class="container" data-layout="inner">
    <div class="row">
      
      <div class="col-xs-2 col_left">
        <%= render :partial => "/layouts/partials/sidebar" %>
      </div>
      
      <div class="col-xs-10 col_right" data-layout="col_right">
        <%= flash_messages %>
        <section class="photos_index">
          <% @photos.each do |photo| %>
            <%= div_for photo, class: :item, data: {ziltag_wrapper: ''} do%>
              <%= image_tag photo.image_url %>
              <% photo.ziltaggings.each do |ziltagging| %>
                <%= link_to '', ziltagging_path(ziltagging, format: :json), remote: true, class: :ziltag_sticker, data: {ziltag_sticker: '', x: ziltagging.x, y: ziltagging.y} %>
              <% end %>
            <% end %>
          <% end %>
        </section><!-- /.find_ziltag_by_img_section -->
      </div>
    </div><!-- /.row -->
  </div><!-- /.container -->
</div><!-- /.container -->
